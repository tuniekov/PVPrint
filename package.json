{
  "name": "pvprint",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && node ./_build/upconfig.js",
    "build:ssr": "vite build --ssr src/entry-server.js --emptyOutDir false",
    "build:component": "vite build && vite build --config vite.config.printbutton.js && node ./_build/upconfig.js",
    "copy": "node ./_build/copy.js",
    "get_token": "node ./_build/get_token.js",
    "upconfig": "node ./_build/upconfig.js",
    "preview": "vite preview",
    "modules:update": "node ./src/modules/generate-index.js"
  },
  "dependencies": {
    "html2pdf.js": "^0.12.1",
    "pvtables": "^0.1.15",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.2",
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.9",
    "dotenv": "^16.3.1",
    "postcss": "^8.4.40",
    "prompt": "^1.3.0",
    "replace-in-file": "^7.0.2",
    "tailwindcss": "^3.4.7",
    "vite": "^5.0.8"
  }
}
